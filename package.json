{
  "name": "cpuutil",
  "displayName": "CPU utilization",
  "description": "Display current CPU utilization",
  "version": "0.2.0",
  "repository": "https://github.com/senier/cpuutil",
  "publisher": "Alexander Senier",
  "license": "Apache-2.0",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "icons": {
      "meter-000": {
        "description": "0%",
        "default": {
          "fontPath": "./res/Meter.woff2",
          "fontCharacter": "K"
        }
      },
      "meter-010": {
        "description": "10%",
        "default": {
          "fontPath": "./res/Meter.woff2",
          "fontCharacter": "J"
        }
      },
      "meter-020": {
        "description": "20%",
        "default": {
          "fontPath": "./res/Meter.woff2",
          "fontCharacter": "I"
        }
      },
      "meter-030": {
        "description": "30%",
        "default": {
          "fontPath": "./res/Meter.woff2",
          "fontCharacter": "H"
        }
      },
      "meter-040": {
        "description": "40%",
        "default": {
          "fontPath": "./res/Meter.woff2",
          "fontCharacter": "G"
        }
      },
      "meter-050": {
        "description": "50%",
        "default": {
          "fontPath": "./res/Meter.woff2",
          "fontCharacter": "F"
        }
      },
      "meter-060": {
        "description": "60%",
        "default": {
          "fontPath": "./res/Meter.woff2",
          "fontCharacter": "E"
        }
      },
      "meter-070": {
        "description": "70%",
        "default": {
          "fontPath": "./res/Meter.woff2",
          "fontCharacter": "D"
        }
      },
      "meter-080": {
        "description": "80%",
        "default": {
          "fontPath": "./res/Meter.woff2",
          "fontCharacter": "C"
        }
      },
      "meter-090": {
        "description": "90%",
        "default": {
          "fontPath": "./res/Meter.woff2",
          "fontCharacter": "B"
        }
      },
      "meter-100": {
        "description": "100%",
        "default": {
          "fontPath": "./res/Meter.woff2",
          "fontCharacter": "A"
        }
      }
    }
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
